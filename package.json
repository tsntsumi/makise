    "autoprefixer": "^10.4.17",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6"
  },
  "dependencies": {
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "framer-motion": "^10.16.16"
  }
}
